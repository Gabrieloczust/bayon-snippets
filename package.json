{
  "name": "vscode-bayon-snippets",
  "publisher": "gabrieloczust",
  "displayName": "Bayon Snippets",
  "description": "Code snippets for React",
  "icon": "art/icon.png",
  "version": "2.0.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gabrieloczust/bayon-snippets/blob/master/README.md"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "Bayon",
    "Bayon React",
    "Bayon Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescript.json"
      }
    ]
  },
  "scripts": {
    "lint": "prettier . --write",
    "build": "node merge.js && vsce package",
    "publish": "yarn build && vsce publish",
    "install-extension": "code --install-extension vscode-bayon-snippet-$npm_package_version.vsix"
  },
  "dependencies": {
    "generator-code": "^1.11.6",
    "glob": "^11.0.1",
    "vsce": "^2.15.0",
    "yo": "^5.1.0"
  },
  "devDependencies": {
    "prettier": "^3.4.2"
  }
}
